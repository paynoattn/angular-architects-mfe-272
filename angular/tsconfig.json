{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2020",
    "jsx": "react",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "lib-shared": [
        "dist/lib-shared"
      ],
      "lib-shared/*": [
        "dist/lib-shared/*"
      ],
      "jszip": [
        "node_modules/jszip/dist/jszip.min.js"
      ],
      "grease": [
        "dist/grease"
      ],
      "grease/*": [
        "dist/grease/*"
      ]
    }
  }
}
